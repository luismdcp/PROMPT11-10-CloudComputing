@model IEnumerable<TaskList>

@using (@Html.BeginFieldset("TaskLists"))
{
    <table class="table table-striped table-bordered table-condensed">
        <thead>
            <tr>
                <th>&nbsp;</th>
                <th>Title</th>
                <th>Created At</th>
            </tr>
        </thead>

        @foreach (var taskList in Model)
        {
            <tr>
                <td>
                    <input type="radio" name="noteCheck" value="@taskList.RowKey" />
                </td>
                <td>@taskList.Title</td>
                <td>@taskList.Timestamp.ToShortDateString()</td>
            </tr>
        }
    </table>       
}